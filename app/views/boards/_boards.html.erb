<tr id="board<%=board.id%>">
  <td>
    <%= link_to board.name, board_path(board) %> 
  </td>
  <td>
    <%= board.board_type%>
  </td>
  <td>
    <%= board.created_at%>
  </td>
  <td>
    <%= link_to edit_board_path(board) do %>
      <i class='fa fa-pencil-square-o' style='font-size:24px'></i>
    <% end %>
    <%= link_to board_path(board), method: :delete, remote: true, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o"></i> 
    <% end %>
  </td>
</tr>
